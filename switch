
pipeline {
   agent any  
   parameters { choice(name: 'DOCKER_REGISTRY', choices: ['PROD', 'QA', 'DEV', 'STAGE', ], description: 'SELECT THE DOCKER REGISTRY ') }
   stages {

      stage('Select docker registry') {

         steps {
              script {
                 def result = ""

switch (DOCKER_REGISTRY) {
    case "DEV":
        result = "DEV"
 powershell
 write-host "dev"
  """

  """
  break

    case "PROD":
        result = "PROD"
 powershell """
  write-host "PROD"
  """
  break

    case "STAGE":
        result = "STAGE"
 powershell """
  write-host "STAGE"
"""
        break

    case "QA":
        result = "QA"
 powershell """
  write-host "QA"
  """
        break

    default:
        result = "DEV"
}
        println result


              }
         }
      }

   }
}
