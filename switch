
pipeline {
    agent any
    parameters { choice(name: '', choices: ['PROD', 'QA', 'DEV', 'STAGE', ], description: 'SELECT THE DOCKER REGISTRY ') }
    stages {
        stage('Select docker registry') {
            steps {
                script {
                    def result = ''

                    switch (DOCKER_REGISTRY) {
                        case 'DEV':
                            result = 'DEV'

                                 agent{ label node}
                                    steps {
                                     script {
                                      powershell '''
                                            write-host "dev"

                                           '''

                                         break
                                     }

                                    }

                        case 'DEV':
                            result = 'pord'

                           agent{ label node}

                                    steps {
                                     script {

                                      powershell '''
                                            write-host "prod"

                                           '''

                       }   }                 break
                 }
            }
      }
 }|
